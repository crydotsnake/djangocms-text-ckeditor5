{
  "name": "djangocms-text-ckeditor",
  "version": "2.0.0",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "lint": "xo",
    "lint:fix": "npm run lint --fix",
    "lint:fix-silent": "npm run lint --silent -- --fix",
    "build:dev": "webpack --mode development",
    "build": "npm-run-all build:dev lint:fix-silent ",
    "start": "echo 'watch onchange'",
    "test:integration": "run-func tests/frontend/index.js runIntegrationTests",
    "test": "npm-run-all lint test:integration"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divio/djangocms-text-ckeditor.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/divio/djangocms-text-ckeditor/issues"
  },
  "homepage": "https://github.com/divio/djangocms-text-ckeditor#readme",
  "dependencies": {
    "@ckeditor/ckeditor5-adapter-ckfinder": "^23.1.0",
    "@ckeditor/ckeditor5-alignment": "^23.1.0",
    "@ckeditor/ckeditor5-autoformat": "^23.1.0",
    "@ckeditor/ckeditor5-basic-styles": "^23.1.0",
    "@ckeditor/ckeditor5-block-quote": "^23.1.0",
    "@ckeditor/ckeditor5-build-classic": "^23.1.0",
    "@ckeditor/ckeditor5-dev-utils": "^23.6.1",
    "@ckeditor/ckeditor5-easy-image": "^23.1.0",
    "@ckeditor/ckeditor5-editor-classic": "^23.1.0",
    "@ckeditor/ckeditor5-essentials": "^23.1.0",
    "@ckeditor/ckeditor5-heading": "^23.1.0",
    "@ckeditor/ckeditor5-image": "^23.1.0",
    "@ckeditor/ckeditor5-link": "^23.1.0",
    "@ckeditor/ckeditor5-list": "^23.1.0",
    "@ckeditor/ckeditor5-paragraph": "^23.1.0",
    "@ckeditor/ckeditor5-theme-lark": "^23.1.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^3.1.0",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "djangocms-casper-helpers": "^4.0.0",
